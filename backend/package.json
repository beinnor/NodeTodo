{
  "name": "NodeTodo",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest"
  },
  "nodemonConfig": {
    "exec": "npm run start",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.2",
    "cors": "^2.8.5",
    "D": "^1.0.0",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "express-jwt": "^6.1.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "sequelize": "^6.16.2"
  },
  "devDependencies": {
    "eslint": "^8.8.0",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15"
  }
}
